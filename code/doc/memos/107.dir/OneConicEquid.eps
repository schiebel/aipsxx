%%Title: OneConicEquid.eps
%%Pages: 1 1
%%DocumentFonts: Courier
%%BoundingBox: 0 0 400 400
%%EndComments

%%BeginProcSet: graphics.pro
   /TextFont /Courier findfont 5 scalefont def

   % Constants.
   /cm2pts 72 2.54 div def
   /pi 3.1416 def
   /w1 0.4 def
   /w2 0.8 def
   /w4 1.6 def

   % Radius of the generating sphere and map boundary.
   /r0 2 cm2pts mul def
   /Rlim 3.3562 r0 mul def

   % Latitude of the standard parallel, and constant of the cone.
   /Theta1 45 def
   /Cone Theta1 sin def
   /a0 Theta1 cos Theta1 sin div Theta1 pi mul 180 div add def

   % Angles in user space at the ends of the parallels.
   /A1 -180 Cone mul 90 sub def
   /A2  180 Cone mul 90 sub def

   /DrawMeridian {
      setlinewidth
      0 0 moveto
      Cone mul
      dup neg rotate
      0 Rlim neg lineto
      exch dup stringwidth
      pop -2 div -7 rmoveto
      show
      rotate
      stroke
      } def

   /DrawParallel {
      setlinewidth
      pi mul 180 div a0 exch sub
      r0 mul dup
      0 0
      3 -1 roll
      A1 A2 arc
      0 exch neg moveto
      2 2 rmoveto
      show
      stroke
      } def
%%EndProcSet

%%EndProlog

userdict /start-hook known {start-hook} if


/vmsave save def
TextFont setfont
2 setlinecap

200 200 translate

  (0)    0 w4 DrawMeridian
   ()   15 w1 DrawMeridian
 (30)   30 w2 DrawMeridian
   ()   45 w1 DrawMeridian
 (60)   60 w2 DrawMeridian
   ()   75 w1 DrawMeridian
 (90)   90 w4 DrawMeridian
   ()  105 w1 DrawMeridian
(120)  120 w2 DrawMeridian
   ()  135 w1 DrawMeridian
(150)  150 w2 DrawMeridian
   ()  165 w1 DrawMeridian
(180)  180 w4 DrawMeridian
(180) -180 w4 DrawMeridian
   () -165 w1 DrawMeridian
(210) -150 w2 DrawMeridian
   () -135 w1 DrawMeridian
(240) -120 w2 DrawMeridian
   () -105 w1 DrawMeridian
(270)  -90 w4 DrawMeridian
   ()  -75 w1 DrawMeridian
(300)  -60 w2 DrawMeridian
   ()  -45 w1 DrawMeridian
(330)  -30 w2 DrawMeridian
   ()  -15 w1 DrawMeridian

1 setgray 0 setlinewidth
0 0 pi 2 div a0 exch sub r0 mul A1 A2 arc
fill stroke
0 setgray

 (90)   90 w4 DrawParallel
   ()   75 w1 DrawParallel
 (60)   60 w2 DrawParallel
   ()   45 w1 DrawParallel
 (30)   30 w2 DrawParallel
   ()   15 w1 DrawParallel
  (0)    0 w4 DrawParallel
   ()  -15 w1 DrawParallel
(-30)  -30 w2 DrawParallel
   ()  -45 w1 DrawParallel
(-60)  -60 w2 DrawParallel
   ()  -75 w1 DrawParallel
(-90)  -90 w4 DrawParallel

vmsave restore
showpage

