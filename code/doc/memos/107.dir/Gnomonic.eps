%%BeginProcSet: graphics.pro
   /TextFont /Courier findfont 5 scalefont def

   % Constants.
   /cm2pts 72 2.54 div def
   /w1 0.4 def
   /w2 0.8 def
   /w4 1.6 def

   % Radius of the generating sphere and map boundary.
   /r0 2 cm2pts mul def
   /Rlim 3.9000 r0 mul def

   /DrawMeridian {
      setlinewidth
      0 0 moveto
      dup neg rotate
      0 Rlim neg lineto
      3 6 rmoveto -3 -6 rlineto -3 6 rlineto 3 -6 rmoveto
      exch dup stringwidth
      pop -2 div -7 rmoveto
      show
      rotate
      stroke
      } def

   /DrawParallel {
      setlinewidth
      dup cos exch sin div
      r0 mul dup
      0 0
      3 -1 roll
      0 360 arc
      0 exch neg moveto
      2 2 rmoveto
      show
      stroke
      } def
%%EndProcSet

%%EndProlog

userdict /start-hook known {start-hook} if


/vmsave save def
TextFont setfont
2 setlinecap

220 220 translate

  (0)   0 w4 DrawMeridian
   ()  15 w1 DrawMeridian
 (30)  30 w2 DrawMeridian
   ()  45 w1 DrawMeridian
 (60)  60 w2 DrawMeridian
   ()  75 w1 DrawMeridian
 (90)  90 w4 DrawMeridian
   () 105 w1 DrawMeridian
(120) 120 w2 DrawMeridian
   () 135 w1 DrawMeridian
(150) 150 w2 DrawMeridian
   () 165 w1 DrawMeridian
(180) 180 w4 DrawMeridian
   () 195 w1 DrawMeridian
(210) 210 w2 DrawMeridian
   () 225 w1 DrawMeridian
(240) 240 w2 DrawMeridian
   () 255 w1 DrawMeridian
(270) 270 w4 DrawMeridian
