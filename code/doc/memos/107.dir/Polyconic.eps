%%BeginProcSet: graphics.pro
   /TextFont /Courier findfont 5 scalefont def

   % Constants.
   /cm2pts 72 2.54 div def
   /pi 3.1416 def
   /w1 0.4 def
   /w2 0.8 def
   /w4 1.6 def

   % Radius of the generating sphere.
   /r0 2 cm2pts mul def


   /DrawMeridian {
      setlinewidth
      /phi exch def
      /theta 90 def
      0 r0 pi mul 2 div moveto
      45 {
         /theta theta 4 sub def
         /a1 theta sin phi mul def
         /a2 theta cos theta sin div def
         a1 sin a2 mul r0 mul
         1 a1 cos sub a2 mul theta pi mul 180 div add r0 mul
         lineto
         } repeat
      phi pi mul 180 div r0 mul 0 moveto
      -2 2 rmoveto
      90 rotate
      show
      -90 rotate
      stroke
      } def

   /DrawParallel {
      setlinewidth
      dup
      dup cos exch sin div
      exch
      dup sin 180 mul abs
      exch pi mul 180 div
      r0 mul dup
      0 exch
      5 -1 roll
      r0 mul dup
      3 -1 roll add exch
      5 -1 roll
      dup neg 90 sub exch
      90 sub
      arc
      0 exch moveto
      2 2 rmoveto
      show
      stroke
      } def

   /DrawEquator {
      setlinewidth
      pi r0 mul dup
      neg 0 moveto
      0 lineto
      2 2 moveto
      show
      stroke
      } def

%%EndProcSet

%%EndProlog

userdict /start-hook known {start-hook} if


/vmsave save def
TextFont setfont
2 setlinecap

185 185 translate

  (0)    0 w4 DrawMeridian
   ()   15 w1 DrawMeridian
 (30)   30 w2 DrawMeridian
   ()   45 w1 DrawMeridian
 (60)   60 w2 DrawMeridian
   ()   75 w1 DrawMeridian
 (90)   90 w4 DrawMeridian
   ()  105 w1 DrawMeridian
(120)  120 w2 DrawMeridian
   ()  135 w1 DrawMeridian
(150)  150 w2 DrawMeridian
   ()  165 w1 DrawMeridian
(180)  180 w4 DrawMeridian
(180) -180 w4 DrawMeridian
   () -165 w1 DrawMeridian
(210) -150 w2 DrawMeridian
   () -135 w1 DrawMeridian
(240) -120 w2 DrawMeridian
   () -105 w1 DrawMeridian
(270)  -90 w4 DrawMeridian
   ()  -75 w1 DrawMeridian
(300)  -60 w2 DrawMeridian
   ()  -45 w1 DrawMeridian
(330)  -30 w2 DrawMeridian
   ()  -15 w1 DrawMeridian

   ()   75 w2 DrawParallel
 (60)   60 w2 DrawParallel
   ()   45 w1 DrawParallel
 (30)   30 w2 DrawParallel
   ()   15 w1 DrawParallel
  (0)      w4 DrawEquator
   ()  -15 w1 DrawParallel
(-30)  -30 w2 DrawParallel
   ()  -45 w1 DrawParallel
(-60)  -60 w2 DrawParallel
   ()  -75 w2 DrawParallel

vmsave restore
showpage


