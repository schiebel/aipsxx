%%BeginProcSet: graphics.pro
   /TextFont /Courier findfont 5 scalefont def

   % Constants.
   /cm2pts 72 2.54 div def
   /pi 3.1416 def
   /w1 0.4 def
   /w2 0.8 def
   /w4 1.6 def

   % Radius of the generating sphere and map boundary.
   /r0 2 cm2pts mul def
   /xlim 3.1416 r0 mul def
   /ylim 2.5000 r0 mul def

   /DrawMeridian {
      setlinewidth
      pi mul 180 div
      r0 mul
      dup ylim neg moveto
      -3 6 rmoveto 3 -6 rlineto 3 6 rlineto -3 -6 rmoveto
      dup ylim lineto
      -3 -6 rmoveto 3 6 rlineto 3 -6 rlineto -3 6 rmoveto
      0 moveto
      -2 2 rmoveto
      90 rotate
      show
      -90 rotate
      stroke
      } def

   /DrawParallel {
      setlinewidth
      90 add 2 div dup sin exch cos div ln
      r0 mul
      dup xlim neg exch moveto
      dup xlim exch lineto
      0 exch moveto
      2 2 rmoveto
      show
      stroke
      } def
%%EndProcSet

%%EndProlog

userdict /start-hook known {start-hook} if


/vmsave save def
TextFont setfont
2 setlinecap

185 150 translate

  (0)    0 w4 DrawMeridian
   ()  -15 w1 DrawMeridian
 (30)  -30 w2 DrawMeridian
   ()  -45 w1 DrawMeridian
 (60)  -60 w2 DrawMeridian
   ()  -75 w1 DrawMeridian
 (90)  -90 w4 DrawMeridian
   () -105 w1 DrawMeridian
(120) -120 w2 DrawMeridian
   () -135 w1 DrawMeridian
(150) -150 w2 DrawMeridian
   () -165 w1 DrawMeridian
(180) -180 w4 DrawMeridian
(180)  180 w4 DrawMeridian
   ()  165 w1 DrawMeridian
(210)  150 w2 DrawMeridian
   ()  135 w1 DrawMeridian
(240)  120 w2 DrawMeridian
   ()  105 w1 DrawMeridian
(270)   90 w4 DrawMeridian
   ()   75 w1 DrawMeridian
(300)   60 w2 DrawMeridian
   ()   45 w1 DrawMeridian
(330)   30 w2 DrawMeridian
   ()   15 w1 DrawMeridian

   ()   75 w1 DrawParallel
 (60)   60 w2 DrawParallel
   ()   45 w1 DrawParallel
 (30)   30 w2 DrawParallel
   ()   15 w1 DrawParallel
  (0)    0 w4 DrawParallel
   ()  -15 w1 DrawParallel
(-30)  -30 w2 DrawParallel
   ()  -45 w1 DrawParallel
(-60)  -60 w4 DrawParallel
   ()  -75 w1 DrawParallel

vmsave restore
