%%BeginProcSet: graphics.pro
   /TextFont /Courier findfont 5 scalefont def

   % Constants.
   /cm2pts 72 2.54 div def
   /w1 0.4 def
   /w2 0.8 def
   /w4 1.6 def

   % Radius of the generating sphere.
   /r0 2 cm2pts mul def
   /r2 2 sqrt r0 mul def

   /DrawMeridian {
      setlinewidth
      90 div
      dup DrawEllipse
      stroke
      r2 mul 0 moveto
      -2 2 rmoveto
      90 rotate
      show
      -90 rotate
      stroke
      } def

   /DrawEllipse {
      /a1 exch def
      /theta 90 def
      0 r2 moveto
      36 {
         /theta theta 5 sub def
         theta cos r2 mul a1 mul
         theta sin r2 mul
         lineto
         } repeat
      } def

   /DrawParallel {
      setlinewidth
      r0 mul dup dup
      2 r2 mul neg exch moveto
      2 r2 mul exch lineto
      0 exch moveto
      2 2 rmoveto
      show
      stroke
      } def

%%EndProcSet

%%EndProlog

