%%BeginProcSet: graphics.pro
   /TextFont /Courier findfont 5 scalefont def

   % Constants.
   /cm2pts 72 2.54 div def
   /pi 3.1416 def
   /w1 0.4 def
   /w2 0.8 def
   /w4 1.6 def

   % Radius of the generating sphere.
   /r0 2 cm2pts mul def
   /r1 r0 pi mul 180 div def

   /DrawMeridian {
      setlinewidth
      DrawSinusoid
      stroke
      0 moveto
      -2 2 rmoveto
      90 rotate
      show
      -90 rotate
      stroke
      } def

   /DrawSinusoid {
      r1 mul neg
      /theta 90 def
      0 theta r1 mul moveto
      36 {
         /theta theta 5 sub def
         dup theta cos mul
         theta r1 mul lineto
         } repeat
      } def

   /DrawParallel {
      setlinewidth
      dup
      cos r1 mul 180 mul
      exch r1 mul
      2 copy dup 0 exch 6 2 roll
      exch neg exch moveto lineto moveto
      2 2 rmoveto
      show
      stroke
      } def

%%EndProcSet

%%EndProlog

userdict /start-hook known {start-hook} if


/vmsave save def
TextFont setfont
2 setlinecap

190 100 translate

  (0)    0 w4 DrawMeridian
   ()   15 w1 DrawMeridian
 (30)   30 w2 DrawMeridian
   ()   45 w1 DrawMeridian
 (60)   60 w2 DrawMeridian
   ()   75 w1 DrawMeridian
 (90)   90 w4 DrawMeridian
   ()  105 w1 DrawMeridian
(120)  120 w2 DrawMeridian
   ()  135 w1 DrawMeridian
(150)  150 w2 DrawMeridian
   ()  165 w1 DrawMeridian
(180)  180 w4 DrawMeridian
(180) -180 w4 DrawMeridian
   () -165 w1 DrawMeridian
(210) -150 w2 DrawMeridian
   () -135 w1 DrawMeridian
(240) -120 w2 DrawMeridian
   () -105 w1 DrawMeridian
(270)  -90 w4 DrawMeridian
   ()  -75 w1 DrawMeridian
(300)  -60 w2 DrawMeridian
   ()  -45 w1 DrawMeridian
(330)  -30 w2 DrawMeridian
   ()  -15 w1 DrawMeridian

   ()   75 w1 DrawParallel
 (60)   60 w2 DrawParallel
   ()   45 w1 DrawParallel
 (30)   30 w2 DrawParallel
   ()   15 w1 DrawParallel
  (0)    0 w4 DrawParallel
   ()  -15 w1 DrawParallel
(-30)  -30 w2 DrawParallel
   ()  -45 w1 DrawParallel
(-60)  -60 w2 DrawParallel
   ()  -75 w1 DrawParallel

vmsave restore
showpage

