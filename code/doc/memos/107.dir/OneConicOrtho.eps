%%BeginProcSet: graphics.pro
   /TextFont /Courier findfont 5 scalefont def

   % Constants.
   /cm2pts 72 2.54 div def
   /w1 0.4 def
   /w2 0.8 def
   /w4 1.6 def

   % Radius of the generating sphere and map boundary.
   /r0 2 cm2pts mul def
   /Rlim 3.9000 r0 mul def

   % Latitude of the standard parallel, and constant of the cone.
   /Theta1 45 def
   /Cone Theta1 sin def
   /a0 Theta1 sin def
   /a1 Theta1 cos Theta1 sin div
       90 Theta1 sub 2 div dup sin exch cos div a0 exp div def

   % Angles in user space at the ends of the parallels.
   /A1 -180 Cone mul 90 sub def
   /A2  180 Cone mul 90 sub def

   /DrawMeridian {
      setlinewidth
      0 0 moveto
      Cone mul
      dup neg rotate
      0 Rlim neg lineto
      3 6 rmoveto -3 -6 rlineto -3 6 rlineto 3 -6 rmoveto
      exch dup stringwidth
      pop -2 div -7 rmoveto
      show
      rotate
      stroke
      } def

   /DrawParallel {
      setlinewidth
      90 exch sub 2 div dup sin exch cos div a0 exp a1 mul
      r0 mul dup
      0 0
      3 -1 roll
      A1 A2 arc
      0 exch neg moveto
      2 2 rmoveto
      show
      stroke
      } def
%%EndProcSet

%%EndProlog

userdict /start-hook known {start-hook} if


/vmsave save def
TextFont setfont
2 setlinecap

230 230 translate

  (0)    0 w4 DrawMeridian
   ()   15 w1 DrawMeridian
 (30)   30 w2 DrawMeridian
   ()   45 w1 DrawMeridian
 (60)   60 w2 DrawMeridian
   ()   75 w1 DrawMeridian
 (90)   90 w4 DrawMeridian
   ()  105 w1 DrawMeridian
(120)  120 w2 DrawMeridian
   ()  135 w1 DrawMeridian
(150)  150 w2 DrawMeridian
   ()  165 w1 DrawMeridian
(180)  180 w4 DrawMeridian
(180) -180 w4 DrawMeridian
   () -165 w1 DrawMeridian
(210) -150 w2 DrawMeridian
   () -135 w1 DrawMeridian
(240) -120 w2 DrawMeridian
   () -105 w1 DrawMeridian
(270)  -90 w4 DrawMeridian
   ()  -75 w1 DrawMeridian
(300)  -60 w2 DrawMeridian
   ()  -45 w1 DrawMeridian
(330)  -30 w2 DrawMeridian
   ()  -15 w1 DrawMeridian

   ()   75 w1 DrawParallel
 (60)   60 w2 DrawParallel
   ()   45 w1 DrawParallel
 (30)   30 w2 DrawParallel
   ()   15 w1 DrawParallel
  (0)    0 w4 DrawParallel
   ()  -15 w1 DrawParallel
(-30)  -30 w2 DrawParallel
   ()  -45 w1 DrawParallel

vmsave restore
showpage

