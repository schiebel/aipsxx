\begin{ahobject}{measuresgui}{gui code for measures.g}

\ahinclude{measuresgui.g}

\ahkeyword{units}{}
\ahkeyword{measures}{}
\ahkeyword{coordinates}{}
\ahkeyword{quantities}{}
\ahkeyword{gui}{}


\begin{ahdescription}

measuresgui provides the code for the GUI interface to the units, quantities
 and measures closure. It cannot be used (or included) on its own.
It will be automatically included and used whenever a GUI is asked for by
the measures closure (using dm.gui()).

\end{ahdescription}

\end{ahobject}
