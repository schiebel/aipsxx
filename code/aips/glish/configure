            eval ${tmp_id}="$SLIB_SUFFIX.$_cv_version".`cat $tmp_index/change.h | head -1 | sed 's/^[^0-9]*\([0-9][0-9]*\).*/\1/'`
            
	else
            eval ${tmp_id}="$SLIB_SUFFIX.$_cv_version".0
            
        fi
    done

fi

