<?xml version="1.0" encoding="UTF-8"?>
<psd:pset xmlns:psd="http://www.aoc.nrao.edu/~sharring/psd.html"  
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="http://www.aoc.nrao.edu/~sharring/psd.html psetdef.xsd">

<!-- This is a sample param set for vla filler -->
<name>vlafillertest</name>
<comment>using vla filler as an example</comment>
   <param xsi:type="psd:string">
<name>msname</name>
<required>true</required>
<prompt>e.g. myvladata.ms</prompt>
<help>name of output ms</help>
</param>
   <param xsi:type="psd:string">
<name>inputfile</name>
<required>true</required>
<prompt>e.g. c477_040502.xpl</prompt>
<help>name of vla archive</help>
</param>
   <param xsi:type="psd:string">
<name>project</name>
<required>false</required>
<prompt>e.g. ad577</prompt>
   <help>
name of project to extract, defaults to all projects in input
</help>
<default>all</default>
</param>
   <param xsi:type="psd:string">
<name>start</name>
<required>false</required>
<prompt>e.g. 2004/05/02/18:30:00</prompt>
<help>start time to extract</help>
<default>blank</default>
</param>
   <param xsi:type="psd:string">
<name>stop</name>
<required>false</required>
<prompt>e.g. 2004/05/02/19:00:00</prompt>
<help>end time of extracted data</help>
<default>blank</default>
</param>
   <param xsi:type="psd:string">
<name>centerfreq</name>
<required>false</required>
<prompt>e.g. 1.489 GHz</prompt>
   <help>
frequency of data to extract (used along with bandwidth param)
</help>
<default>blank</default>
</param>
   <param xsi:type="psd:string">
<name>bandwidth</name>
<required>false</required>
<prompt>e.g. 10.01 Mhz</prompt>
<help>data around centerfreq to get out</help>
<default>0.0</default>
</param>
   <param xsi:type="psd:string">
<name>bandname</name>
<required>false</required>
<prompt>e.g. P</prompt>
<help>name of band to extract</help>
<default>all</default>
<length>10</length>
</param>
   <param xsi:type="psd:string">
<name>source</name>
<required>false</required>
<prompt>e.g. 3C49 or 1338+4995</prompt>
<help>name of source</help>
<default>all</default>
</param>
   <param xsi:type="psd:bool">
<name>overwrite</name>
<required>false</required>
<prompt>e.g. true or false</prompt>
<help>overwrite or append</help>
<default>false</default>
</param>
</psd:pset>

